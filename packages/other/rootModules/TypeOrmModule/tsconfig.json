{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "strict": true,
      "removeComments": false,
      "noLib": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "ES2022",
      "sourceMap": false,
      "outDir": "dist",
      "skipLibCheck": true,
      "useUnknownInCatchVariables": false
    },
    "include": [
      "lib/**/*"
    ],
    "exclude": [
      "node_modules",
    ]
  }